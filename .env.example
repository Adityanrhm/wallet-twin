# Wallet Twin Environment Variables
# Copy this file to .env and fill in your values

# Database Configuration
WT_DATABASE_HOST=localhost
WT_DATABASE_PORT=5432
WT_DATABASE_USER=postgres
WT_DATABASE_PASSWORD=your_password_here
WT_DATABASE_NAME=wallet_twin
WT_DATABASE_SSL_MODE=disable

# Application Settings
WT_APP_NAME=Wallet Twin
WT_APP_CURRENCY=IDR
WT_APP_LOCALE=id-ID

# TUI Settings
WT_TUI_THEME=default
WT_TUI_REFRESH_RATE=1000
